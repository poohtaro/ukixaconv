#!/bin/sh
set -eu
sed 's/｜《/※［＃始め二重山括弧、1-1-52］/g' |
sed 's/｜》/※［＃終わり二重山括弧、1-1-53］/g' |
sed 's/｜［/※［＃始め角括弧、1-1-46］/g' |
sed 's/｜］/※［＃終わり角括弧、1-1-47］/g' |
sed 's/｜〔/※［＃始めきっこう（亀甲）括弧、1-1-44］/g' |
sed 's/｜〕/※［＃終わりきっこう（亀甲）括弧、1-1-45］/g' |
sed 's/｜｜/※［＃縦線、1-1-35］/g' |
sed 's/｜＃/※［＃井げた、1-1-84］/g' |
sed 's/｜※/※［＃米印、1-2-8］/g' |
sed 's/｜＜/※［＃不等号（小なり）、U+FF1C、00-00］/g' |
sed 's/｜＞/※［＃不等号（大なり）、U+FF1E、00-00］/g'
